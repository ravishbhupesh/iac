{
    "project": "idoc",
    "company": "CMA-CGM",
    "aws_region": "eu-west-1",
    "naming_prefix": "idoc",
    "environment": "dev",
    "rds" : {
        "rds_db_identifier": "idocidt",
        "rds_allocated_storage": 5,
        "rds_db_engine": "postgres",
        "rds_db_engine_version": "14.7",
        "rds_db_instance_class": "db.t3.micro",
        "rds_db_name": "defaultdb",
        "rds_db_username": "defaultuser",
        "rds_db_port": 5432,
        "rds_create_random_password": false,
        "rds_maintenance_window": "Mon:00:00-Mon:03:00",
        "rds_backup_window": "03:00-06:00",
        "rds_create_monitoring_role": true,
        "rds_aws_monitoring_interval": 30,
        "rds_create_db_subnet_group": true,
        "rds_db_family": "postgres14",
        "rds_publicly_accessible": true,
        "rds_aws_deletion_protection": false,
        "rds_aws_performance_insights_enabled": false,
        "rds_aws_performance_insights_retention_period": 7,
        "backup_retention_period": 7,
        "rds_cloudwatch_log_group_retention_in_days": 7
    }
}